{
    "extends": "eslint-config-globex",
    "overrides": [{
        files: ["pages/*.js"],
        rules: {
            "filenames/match-exported": "off"
        }
    }],
    "rules": {
        "jsx-a11y/anchor-is-valid": "off",
        "jsx-a11y/html-has-lang": "off",
        "no-process-env": "off",
        "promise/no-native": "off"
    }
}
